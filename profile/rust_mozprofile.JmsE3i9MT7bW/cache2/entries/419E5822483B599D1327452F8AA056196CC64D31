/*************************************************************/
var IV_JCT = "";

function getJunction() {
	return IV_JCT;
}

var DEFAULT_IV_JCT = "";
var KNOWN_IV_JCT = new Array();
KNOWN_IV_JCT[0] = "/oys";  	// salesnet production
KNOWN_IV_JCT[1] = "/oysdev"; 	// WAS 6.0 development
KNOWN_IV_JCT[2] = "/oystst"; 	// WAS 6.0 test
KNOWN_IV_JCT[3] = "/oysqa"; 	// WAS 6.0 QA
KNOWN_IV_JCT[4] = "/oyslnx"; 	// prod
KNOWN_IV_JCT[5] = "/oyssand"; 	// WAS 6.0 development
KNOWN_IV_JCT[6] = "/oyssand8"; 	// WAS 8.0 development
KNOWN_IV_JCT[7] = "/oystst8"; 	// WAS 8.0 test
KNOWN_IV_JCT[8] = "/oysqa8"; 	// WAS 8.0 QA


var search = "IV_JCT";
var junction = "";
//Browser detection functions
var isIE = false;
var isIE6 = false;

	if (/MSIE[\/\s](\d+\.\d+)/.test(navigator.userAgent)){ //test for MSIE 8.0; ignoring remaining digits;
		isIE = true;
		var IEversion=new Number(RegExp.$1);
		if(IEversion < 7){
			isIE6 = true;
		}
	}
	
	
	//Detecting applet window size
	function getComputedHeight()
	{ 
		var viewportheight;
	
		if (typeof window.innerWidth != 'undefined')
		{
			viewportheight = window.innerHeight;
		}
  
		// IE6 in standards compliant mode (i.e. with a valid doctype as the first line in the document)
 
		else if (typeof document.documentElement != 'undefined'
			&& typeof document.documentElement.clientWidth !='undefined' && document.documentElement.clientWidth != 0)
		{
			viewportheight = document.documentElement.clientHeight;
		}
  
		// older versions of IE
  
		else
		{
			viewportheight = document.getElementsByTagName('body')[0].clientHeight;
		}
		return viewportheight;
	}
	function getComputedWidth(theElt)
	{
		var viewportwidth;
	
		if (typeof window.innerWidth != 'undefined')
		{
			viewportwidth = window.innerWidth;
		}
  
		// IE6 in standards compliant mode (i.e. with a valid doctype as the first line in the document)
 
		else if (typeof document.documentElement != 'undefined'
			&& typeof document.documentElement.clientWidth !='undefined' && document.documentElement.clientWidth != 0)
		{
			viewportwidth = document.documentElement.clientWidthp;
		}
  
		// older versions of IE
  
		else
		{
			viewportwidth = document.getElementsByTagName('body')[0].clientWidth;
		}
		return viewportwidth;
	}
if (document.cookie.length > 0) { 
	// if there are any cookies      
	offset = document.cookie.indexOf(search);
	if (offset != -1) { 
		// if cookie exists          
		offset = document.cookie.indexOf("=", offset);
		// set index of beginning of value
		if (offset != -1) {
			// add the length of = sign
			offset += 1;
			end = document.cookie.indexOf(";", offset);
			// set index of end of cookie value         
			if (end == -1)             
				end = document.cookie.length;
			junction = unescape(document.cookie.substring(offset, end));
		}
	}    
}
// trim string
junction = junction.replace(/^\s*|\s*$/g,"");
if( junction.length == 0 ) {
	IV_JCT = DEFAULT_IV_JCT;
}
else {
	// To be safe, let's check for the known junctions
	for(var i=0;i<KNOWN_IV_JCT.length;i++) {
		if( KNOWN_IV_JCT[i] == junction ) {
			IV_JCT = junction;
			break;
		}
		if( i >= KNOWN_IV_JCT.length-1 ) {
			IV_JCT = DEFAULT_IV_JCT;
		}
	}
}
/*************************************************************/
var pageModified = false;
function setPageModified(modified) {

	pageModified = modified;
}
function isPageModified() {

	return pageModified;
}
/*************************************************************/
function onTemplateLoad() {
	InitializeTimer(true)
	if (window.onAlertMessage != null) {	
		onAlertMessage();
	}
	if (window.onContentLoad != null) {	
		onContentLoad();
	}
	////////////////////////////
	if (window.setBannerMenu != null) {	
		setBannerMenu();
	}
	if(window.onresize != null){
		window.resize();
	}
	
	//REQ000001689926_RMS NonSupported Browser Messaging - START
	
	var url = window.location.pathname;
	
	var n = url.lastIndexOf('/');
	
	var result = url.substring(n + 1);
	
	var browsername="NA";
	
	if(result==="signIn"){
	
		if (navigator.appName=="Netscape") {
			
			if (navigator.userAgent.indexOf("Firefox")>-1) {
				
				browsername="FIREFOX";
				/*if(navigator.userAgent.indexOf("48") > -1){
					browsername="FIREFOX";
				}	*/
			}
			
			if (navigator.userAgent.indexOf("11")> -1) {
				browsername="IE";
			}
			
		}
		
		
		if(browsername==="NA"){
		
			alert('Marriott \'s One Yield application is supported only by Microsoft Internet Explorer 11 and Firefox browser.You may experience difficulties in accessing certain features of the application in your current browser.');
			
		}
	}
	
	
	//REQ000001689926_RMS NonSupported Browser Messaging - END
	
}
/*************************************************************/
function onClickLink( objLink, sUrl , bCheckModified) {
	if( !confirmPageModified(bCheckModified) ) {
		return false;
	}
	theWindow = openWindow(sUrl, objLink.target, "");
	return false;
}

function onClickLinkOpenWithinWindow(sUrl, bCheckModified) {
	if( !confirmPageModified(bCheckModified) ) {
		return false;
	}
	location.href = sUrl;
}

function onClickLinkSubmitForm( submittedForm, bCheckModified ) {
	if( !confirmPageModified(bCheckModified) ) {
		return false;
	}
	submittedForm.submit();
	return false;
}
function onChangeSelectSubmitForm( selectControl, bCheckModified ) {
	if( !confirmPageModified(bCheckModified) ) {
		for (var i = 0; i < selectControl.length; i++) {
			    if (selectControl.options[i].defaultSelected == true) {
			          selectControl.options[i].selected=true      
			}   
		}
		return false;
	}
	selectControl.form.submit();
	return false;
}
function onClickLinkSubmitFormOverrideAction( submittedForm, sAction, bCheckModified ) {
	if( !confirmPageModified(bCheckModified) ) {
		return false;
	}
	prefixedUrl = applyPathPrefix(sAction);
	defaultAction = submittedForm.action;
	submittedForm.action = prefixedUrl;
	submittedForm.submit();
	submittedForm.action = defaultAction;
	return false;
}
function onClickLinkSubmitFormOpenPrint( submittedForm, sAction, target ) {
	var reportOptions = "resizable, toolbar=false, location=false, directories=false, menubar=false, height=500, width=700";
	theWindow = openWindow("", target, reportOptions);
	prefixedUrl = applyPathPrefix(sAction);
	defaultAction = submittedForm.action;
	defaultTarget = submittedForm.target;
	submittedForm.action = prefixedUrl;
	submittedForm.target = target;
	submittedForm.submit();
	submittedForm.action = defaultAction;
	submittedForm.target = defaultTarget;
	return false;
}
function onClickLinkSubmitFormOpenWindow( submittedForm, sAction, target ) {
	var reportOptions = "resizable, toolbar=false, location=false, directories=false, menubar=false, height=300, width=500";
	theWindow = openWindow("", target, reportOptions);
	prefixedUrl = applyPathPrefix(sAction);
	defaultAction = submittedForm.action;
	defaultTarget = submittedForm.target;
	submittedForm.action = prefixedUrl;
	submittedForm.target = target;
	submittedForm.submit();
	submittedForm.action = defaultAction;
	submittedForm.target = defaultTarget;
	return false;
}
function onClickLinkOpenReport( objLink, sUrl ) {
	var reportOptions = "resizable, toolbar=false, location=false, directories=false, menubar=false, height=500, width=700";
	theWindow = openWindow(sUrl, objLink.target, reportOptions);
	return false;
}
function onClickLinkOpenHelp( objLink, sUrl ) {
	var helpOptions = "resizable, toolbar=false, location=false, directories=false, menubar=false, height=625, width=700";
	theWindow = openWindowWithOutPrefixAndTimer(sUrl, objLink.target, helpOptions);
	return false;
}
function onClickLinkOpenStatus( objLink, sUrl ) {
	var onlineStatusOptions = "toolbar=false, location=false, directories=false, menubar=false, height=120, width=320";
	theWindow = openWindow(sUrl, objLink.target, onlineStatusOptions);
	return false;
}
function onClickLinkOpenSupport( objLink, sUrl ) {
	var supportOptions = "toolbar=false, location=false, directories=false, menubar=false, height=375, width=600";
	theWindow = openWindow(sUrl, objLink.target, supportOptions);
	return false;
}
function applyPathPrefix(sUrl) {

	if( sUrl.length != 0 ) {
		return getJunction()+sUrl;
	}
	else {
		return sUrl;
	}
}
function filterWindowName(target) {
	var targetName = "";
	var c;
	if(target != null && target.length > 0) {
		for(var i = 0; i < target.length; i++) {
			c = target.charAt(i);
			if((c >= '0' && c <= '9') || (c >= 'A' && c <= 'Z') || (c >= 'a' && c <= 'z') || c == '_') {
				targetName += c;
			} else {
				targetName += '0';
			}
		}
	} else{
		targetName = target;
	}
	return targetName;
}
function openWindow( sUrl, target ,options) {
	InitializeTimer(true);
   	prefixedUrl = applyPathPrefix(sUrl);
	var name = filterWindowName(target);
	theWindow = window.open(prefixedUrl, name, options);
	theWindow.focus();
	return theWindow;
}

function openWindowWithOutPrefixAndTimer( sUrl, target ,options) {
	var name = filterWindowName(target);
	theWindow = window.open(sUrl, name, options);
	theWindow.focus();
	return theWindow;
}

function onClickLinkOpenSubmitReportForm( submittedForm, bCheckModified ) {
	if( !confirmPageModified(bCheckModified) ) {
		return false;
	}
	var reportOptions = "resizable, toolbar=false, location=false, directories=false, menubar=false, height=500, width=700";
	theWindow = openWindow("", submittedForm.target, reportOptions);
	submittedForm.submit();
	return false;
}
function confirmPageModified(bCheckModified) {
	//This is to verify whether THC app is opened in update mode and then release the lock.
	verifyAndReleaseTHCLock();
	if( bCheckModified ) {
		if( isPageModified() ) {
			if( !confirm("You have unsaved changes!\n\nDo you want to discard all your changes?") ) {
			
				return false;
			}
		}
	}
	return true;
}
function onClickNotImplemented()
{
    alert("Not Implemented");
    return false;
}
function onFormSubmitNotAllowed() {

	return false;
}

//JSON functions
function getXHttp()
{
	xhttp=null;
	
	if(window.XMLHttpRequest) 
	{
		xhttp = new XMLHttpRequest();
	} else {
		if(window.ActiveXObject) {
			xhttp = new ActiveXObject('MSXML2.XMLHTTP.3.0');
		}
	}
	
	return xhttp;
}

function getServerCookies(servletPath)
{
	xhttp = getXHttp();
	xhttp.open("GET",servletPath,false);
	xhttp.send("");
	jsonDoc = xhttp.responseText;
	return jsonDoc;
	
}
/*************************************************************/

/*Following functions are used in new THC (totalHotelCalendarContent.jsp)*/
function makeAjaxServerCall(servletPath)
{
	xhttp = getXHttp();
	xhttp.open("GET",servletPath,false);
	xhttp.send("");
	jsonDoc = xhttp.responseText;
	return jsonDoc;
	
}

var thcAccessMode = "read";
var thcUnlockUrl = "";

function setTHCAccessMode(accessMode) {
	thcAccessMode = accessMode;
}

function isTHCOpenedInUpdateMode() {
	return thcAccessMode != "R";
}
/*
 * Users can open THC App in update mode - which means the property is locked and nobody else can access that property 
 * unless the lock is released. This function is to verify whether the THC app is opened in an update mode and make an 
 * AJAX call to THC App to release the lock. 
 * */
function verifyAndReleaseTHCLock(){
	if( isTHCOpenedInUpdateMode() && thcUnlockUrl != ''){
		console.log("THC was opened in update mode..Releasing the lock.");
		makeAjaxServerCall(thcUnlockUrl);
	}
}
